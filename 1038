code1,code2=map(int,input().split())
total1=0
total2=0
total3=0
total4=0
total5=0
if  code1==1:
    total1+=4.00
elif code1==2:
    total2+=4.50
elif code1==3:
    total3+=5.00
elif code1==4:
    total4+=2.00
elif code1==5:
    total5+=1.50
totalg=(total1+total2+total3+total4+total5)*code2
print("Total: R$ %.2f"%totalg)
