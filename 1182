C=int(input())
T=input()
matriz=[[float(input())for i in range(12)]for j in range(12)]
soma=0
for i in range(12):
    for j in range(12):
        if C==j:
            if T=='S':
                soma+=matriz[i][j]
            else:
                soma+=matriz[i][j]
                media=soma/12
if T=='S':
    print('{:.1f}'.format(soma))
else:
    print('{:.1f}'.format(media))
