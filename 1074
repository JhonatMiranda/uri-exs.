num=int(input())
for x in range(num):
    numero=int(input())
    if numero==0:
        print("NULL")
    if numero%2==0 and numero>0:
        print("EVEN POSITIVE")
    if numero%2==0 and numero<0:
        print("EVEN NEGATIVE")
    if numero%2!=0 and numero>0:
        print("ODD POSITIVE")
    if numero%2!=0 and numero<0:
        print("ODD NEGATIVE")
