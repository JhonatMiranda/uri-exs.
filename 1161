while True:
    try:
        fat1=1
        fat2=1
        x,y=map(int,input().split())
        if x==0 or y==0:
            fat1=1
            fat2=1
        for i in range(1,x+1):
            fat1*=i
        for j in range(1,y+1):
            fat2*=j
        print(fat1+fat2)
    except EOFError:
        break
