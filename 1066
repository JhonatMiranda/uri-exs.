vetor=[int(input())for x in range(5)]
imp=0
par=0
pos=0
neg=0
for i in range(5):
    if vetor[i]%2!=0 and vetor[i]!=0:
        imp+=1
    if vetor[i]%2==0:
        par+=1
    if vetor[i]>0:
        pos+=1
    if vetor[i]<0:
        neg+=1
print(par,"valor(es) par(es)")
print(imp,"valor(es) impar(es)")
print(pos,"valor(es) positivo(s)")
print(neg,"valor(es) negativo(s)")
