T=input()
matriz=[[float(input())for i in range(12)]for j in range(12)]
soma=0
for i in range(12):
    for j in range(12):
        if i+j<11:
            if T=='S':
                soma+=matriz[i][j]
            else:
                soma+=matriz[i][j]
                media=soma/66
if T=='S':
    print('%.1f'%soma)
else:
    print('%.1f'%media)
