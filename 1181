L=int(input())
T=input()
matriz=[[float(input())for i in range(12)]for j in range(12)]
soma=0
for i in range(12):
    for j in range(12):
        if L==i:
            if T=='S':
                soma+=matriz[i][j]
            else:
                soma+=matriz[i][j]
                media=soma/12
if T=='S':
    print(soma)
else:
    print(media)
